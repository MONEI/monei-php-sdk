{
  "name": "monei-php-sdk",
  "version": "2.7.0",
  "description": "MONEI PHP SDK",
  "author": "monei",
  "type": "module",
  "scripts": {
    "build": "openapi-generator-cli generate --generator-key monei-php && yarn postbuild",
    "build:local": "openapi-generator-cli generate --generator-key monei-php-local && yarn postbuild",
    "release": "release-it",
    "cs-fix": "composer cs-fix",
    "test": "composer test",
    "scope": "./scripts/scope-dependencies.sh",
    "postbuild": "yarn scope && yarn cs-fix && yarn test",
    "prepare": "husky",
    "commitlint": "commitlint --edit"
  },
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@openapitools/openapi-generator-cli": "2.22.0",
    "@release-it/bumper": "^7.0.5",
    "@release-it/conventional-changelog": "^10.0.1",
    "husky": "^9.1.7",
    "release-it": "^19.0.4"
  },
  "packageManager": "yarn@4.9.3"
}
